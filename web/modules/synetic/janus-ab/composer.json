{
  "name": "synetic/janus-ab",
  "description": "Wrap around the JanusAB library in Drupal 8",
  "type": "synetic-module",
  "license": "GPL-2.0+",
  "repositories": [{
    "type": "composer",
    "url": "https://packagist.synetic.nl"
  }],
  "require": {
    "drupal/core":                           "~8.5",
    "synetic/janus-ab-lib":                  "^1.1.0",
    "theiconic/php-ga-measurement-protocol": "^2.7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\janus_ab\\":           "src/",
      "Drupal\\janus_render_cache\\": "modules/janus_render_cache/src/",
      "Drupal\\janus_page_cache\\":   "modules/janus_page_cache/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\janus_ab\\":           "tests/",
      "Drupal\\janus_render_cache\\": "modules/janus_render_cache/tests/",
      "Drupal\\janus_page_cache\\":   "modules/janus_page_cache/tests/"
    }
  },
  "minimum-stability": "dev"
}
